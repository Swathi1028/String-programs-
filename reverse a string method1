import java.util.*;
public class ReverseString {
    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        String s1 = sc.nextLine();
        char ch;
        String r = "";
        for(int i=0;i<s1.length();i++){
            ch = s1.charAt(i);
            r = ch+r;
        }
        System.out.println(r);
        sc.close();
    }
}
input:
parrot
output:
torrap
